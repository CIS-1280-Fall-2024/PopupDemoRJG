<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PopupDemo.ToDoListPage"
             Title="ToDoListPage">
    <VerticalStackLayout>
        <Label 
            Text="Welcome to ToDo page"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />

        <ListView x:Name="toDoListView" ItemsSource="{Binding todos}">
        </ListView>
    </VerticalStackLayout>
</ContentPage>